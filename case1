//try case
import java.util.Scanner;
class Case1 
{
	public static void main(String[] args) 
	{
		Scanner sc=new Scanner(System.in);
		int num1,num2,result=0;
		System.out.println("enter the num1 value");
		num1=sc.nextInt();
		System.out.println("enter the num1 value");
		num2=sc.nextInt();
		try
		{
			result=num1/num2;
			System.out.println("the result is: "+result);
			
		}
		
		
	}
}


