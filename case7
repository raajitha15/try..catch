//try...catch case
import java.util.Scanner;
class Case7
{
	public static void main(String[] args) 
	{
		int num1,num2,num3,result1=0,result2=0;
		Scanner sc=new Scanner(System.in);
		System.out.println("enter the num1 value");
		num1=sc.nextInt();
		System.out.println("enter the num2 value");
		num2=sc.nextInt();
		System.out.println("enter the num3 value");
		num3=sc.nextInt();
		try
		{
			result1=num1/num2;
			System.out.println("the result are: "+result1);
		}
		catch(Exception e)
		{
			System.out.println("result is not valid ");
		}
		try
		{
			result2=num2/num3;
			System.out.println("the result are: "+result2);
		}
		catch(Exception e1)
		{
			System.out.println("result not found");
		}
		
	}
}

//it is executed....

/* error: exception is already declared*/


